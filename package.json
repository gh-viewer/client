    "react-art": "16.3.1",
    "react-dom": "16.3.1",
    "react-native": "^0.55.3",
    "react-native-electron": "^0.5.1",
    "react-native-elements": "^0.19.1",
    "react-native-vector-icons": "^4.6.0",
    "react-native-web": "^0.6.1",
    "react-navigation": "^1.5.11",
    "react-redux": "^5.0.7",
    "react-relay": "^1.5.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "relay-runtime": "^1.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
