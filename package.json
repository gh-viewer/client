{
  "name": "GHViewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "desktop": "electron ./desktop/main.js",
    "webpack": "webpack --progress",
    "webpack-server": "webpack-dev-server --hot --progress",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "relay-compile": "relay-compiler --schema schema.graphql --src src",
    "relay-watch": "yarn run relay-compile -- --watch",
    "relay-schema": "graphql-fetch-schema --graphql https://api.github.com/graphql?access_token=$ACCESS_TOKEN",
    "test": "jest"
  },
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "electron": "^1.6.11",
    "prop-types": "^15.0.0",
    "react": "~15.4.1",
    "react-dom": "~15.4.2",
    "react-native": "0.42.3",
    "react-native-electron": "^0.0.17",
    "react-native-elements": "^0.15.0",
    "react-native-vector-icons": "^4.2.0",
    "react-native-web": "^0.0.113",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "react-relay": "^1.1.0",
    "redux": "^3.7.2",
    "redux-persist": "^4.8.2",
    "relay-runtime": "^1.1.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-preset-react-native": "2.1.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.38.0",
    "graphql-fetch-schema": "^0.6.1",
    "jest": "20.0.4",
    "react-test-renderer": "~15.4.1",
    "relay-compiler": "^1.1.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
