{
  "name": "GHViewer",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "desktop": "electron ./desktop/main.js",
    "webpack": "webpack --progress",
    "webpack-server": "webpack-dev-server --hot --progress",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "relay-compile": "relay-compiler --schema schema.graphql --src src",
    "relay-watch": "yarn run relay-compile -- --watch",
    "relay-schema":
      "graphql-fetch-schema --graphql https://api.github.com/graphql?access_token=$ACCESS_TOKEN",
    "test": "jest"
  },
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "electron": "~1.7.11",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-native": "^0.52.1",
    "react-native-electron": "^0.3.0",
    "react-native-elements": "^0.19.0",
    "react-native-vector-icons": "^4.5.0",
    "react-native-web": "^0.3.2",
    "react-navigation": "^1.0.0-beta.26",
    "react-redux": "^5.0.6",
    "react-relay": "^1.4.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.5.0",
    "relay-runtime": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-react-native": "^4.0.0",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.61.0",
    "graphql-fetch-schema": "^0.7.2",
    "jest": "22.1.4",
    "react-test-renderer": "^16.2.0",
    "relay-compiler": "^1.4.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
